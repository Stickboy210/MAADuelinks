{
  "controller": [
    {
      "name": "安卓端",
      "type": "Adb"
    },
    {
      "name": "桌面端",
      "type": "Win32",
      "win32": {
        "class_regex": ".*",
        "window_regex": "Visual Studio"
      }
    }
  ],
  "resource": [
    {
      "name": "决斗链接服",
      "path": [
        "{PROJECT_DIR}/resource"
      ]
    }
  ],
  "task": [
    {
      "name": "刷主页人机直到体力清空（目前识别可能会翻车，但最后一般能把人机刷完）",
      "entry": "HomePage",
      "option": [
        "主页世界"
      ]
    },
    {
      "name": "清战队任务加战队副本",
      "entry": "FindClan"
    },
    {
      "name": "清自动传送门",
      "entry": "PortalsEntry",
      "option": [
        "传送门等级"
      ]
    },
    {
      "name": "手动清传送门（需要手动停止，卡组带没效果的高攻能通招的怪）",
      "entry": "ManualPortalsEntry",
      "option": [
        "传送门钥匙种类"
      ]
    },
    {
      "name": "领任务",
      "entry": "HomePageReward"
    },
    {
      "name": "刷转轮活动",
      "entry": "WheelActivityEntry"
    }
  ],
